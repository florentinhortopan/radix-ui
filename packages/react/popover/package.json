{
  "name": "@interop-ui/react-popover",
  "version": "0.0.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build src/index.ts && yarn build:styles",
    "build:styles": "ts-node -r tsconfig-paths/register ../../../scripts/build-styles popover",
    "clean": "rm -rf dist",
    "bump": "yarn version --deferred",
    "prepublish": "yarn clean && yarn build"
  },
  "dependencies": {
    "@interop-ui/popper": "workspace:*",
    "@interop-ui/react-arrow": "workspace:*",
    "@interop-ui/react-debug-context": "workspace:*",
    "@interop-ui/react-portal": "workspace:*",
    "@interop-ui/react-use-size": "workspace:*",
    "@interop-ui/react-utils": "workspace:*",
    "@interop-ui/utils": "workspace:*",
    "csstype": "^2.6.11"
  },
  "devDependencies": {
    "parcel": "^2.0.0-beta.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  }
}
