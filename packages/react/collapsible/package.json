{
  "name": "@interop-ui/react-collapsible",
  "version": "0.0.1-5",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "development": "dist/pkg.development.js",
  "module": "dist/pkg.module.min.js",
  "types": "dist/index.d.ts",
  "targets": {
    "development": {
      "minify": false,
      "outputFormat": "global",
      "scopeHoist": false,
      "isLibrary": true
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:prod && yarn build:dev && yarn build:styles && yarn build:entry",
    "build:prod": "parcel build src/index.ts --no-cache",
    "build:dev": "NODE_ENV=development parcel build src/index.ts --no-cache --target development",
    "build:styles": "node ../../../scripts/build-styles collapsible",
    "build:entry": "yarn build:entry:rename && cp ../../../scripts/entry.js dist/index.js",
    "build:entry:rename": "mv dist/index.js dist/pkg.production.min.js && mv dist/index.js.map dist/pkg.production.min.js.map",
    "clean": "rm -rf dist",
    "version": "yarn version",
    "prepublish": "yarn clean && yarn build"
  },
  "dependencies": {
    "@interop-ui/react-utils": "workspace:*",
    "@interop-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "parcel": "^2.0.0-beta.1"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
